{
  "name": "trie",
  "version": "1.0.0",
  "description": "A simple trie Implementation in JavaScript",
  "main": "app.js",
  "scripts": {
    "test": "mocha --reporter mochawesome",
    "test:dev": "mocha --reporter mochawesome --reporter-options reportDir=test/mochawesome --watch",
    "test:browser": "live-server --port=9000 --open=/test/mochawesome/mochawesome.html",
    "test:suite": "concurrently \"npm run test:dev\" \"npm run test:browser\" "
  },
