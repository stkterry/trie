    "type": "git",
    "url": "git+https://github.com/stkterry/trie.git"
  },
  "keywords": [
    "Trie",
    "Node",
    "JavaScript"
  ],
  "author": "Steven K Terry <stkterry@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stkterry/trie/issues"
  },
  "homepage": "https://github.com/stkterry/trie#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "concurrently": "^5.0.2",
    "live-server": "^1.2.1",
    "mocha": "^10.2.0",
    "mochawesome": "^4.1.0"
  }
}
